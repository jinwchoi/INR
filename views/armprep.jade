//Sit with your back against the chair. Place your right arm in the start position. Press the green button to continue.
html
    head
        title=title
        link(href='/css/style.css', rel='stylesheet')
        link(href='/css/circle.css', rel='stylesheet')
        script(src='/socket.io/socket.io.js')
        script(src='/js/task.js')
        script(src='http://code.jquery.com/jquery-1.11.3.min.js')
        script(src='http://code.jquery.com/mobile/1.4.5/jquery.mobile-1.4.5.min.js')
        script(src='/js/featuredisable.js')
        //script(src='/js/papaparse.js');
        script(type='text/javascript')
        script(src='/js/armprep.js')
        div.head
            p#headerContent 
        div.notice
            p#noticeContent
        body(margin="auto")
            div#centeredPatient
                div#taskArea
                    div.centered
                        span#prep Sit with your back against the chair. Place your right arm in the start position. Press the green button to continue.

            div(onclick="move()")#start
                div.centered
                    div.forward
                        img(src="../img/icons-all/forward.png")

            script.
                function move() {
                    sendresponse()
                }
